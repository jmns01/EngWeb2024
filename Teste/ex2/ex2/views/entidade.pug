extends layout

block content
  .w3-card-4 
    header.w3-container.w3-teal
      h1 
        span #{title} 
    .w3-container
      table.w3-table-all
        tr 
          th ID
          th Objecto de Contrato 
          th Data de Celebracao do Contrato
          th Preco Contratual
        each elem in lista
          tr 
            td
              a(href='/' + elem._id) #{elem._id} 
            td = elem.tipoprocedimento
            td= elem.objectoContrato
            td= elem.dataCelebracaoContrato
            td= elem.precoContratual

    footer.w3-container.w3-teal
      h5 Generado por EngWeb2024 in #{date}